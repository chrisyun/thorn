<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
    PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

	<settings>
		<setting name="cacheEnabled" value="true"/>
		<setting name="lazyLoadingEnabled" value="true"/>
		<setting name="multipleResultSetsEnabled" value="true"/>
		<setting name="useColumnLabel" value="true"/>
		<setting name="useGeneratedKeys" value="false"/><!--
		<setting name="enhancementEnabled" value="false"/>
		--><setting name="defaultExecutorType" value="SIMPLE"/>
		<setting name="defaultStatementTimeout" value="25000"/>
	</settings>
	
	<!-- 
	<typeAliases>
		<typeAlias alias="resPage" type="org.cy.thorn.core.entity.ResultPage" />
		<typeAlias alias="resSet" type="org.cy.thorn.core.entity.ResultSet" />
	</typeAliases>
	-->
	
	<mappers>
		 <mapper resource="org/thorn/dao/mapper/MySQLCommonMapper.xml"/> 
		 <mapper resource="org/thorn/dao/mapper/OracleCommonMapper.xml"/> 
	</mappers>
	  
</configuration>
